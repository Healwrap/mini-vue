{
  "name": "mini-vue",
  "version": "1.0.0",
  "description": "模仿Vue的简单实例项目",
  "type": "module",
  "main": "main.ts",
  "scripts": {
    "clear": "rimraf build",
    "build": "npm run clear && rollup -c",
    "publish": "npm run build && npm publish --access public",
    "publish-patch": "node scripts/publish.js patch && npm run publish",
    "publish-minor": "node scripts/publish.js minor && npm run publish",
    "publish-major": "node scripts/publish.js major && npm run publish",
    "format": "prettier --write ./"
  },
  "keywords": [],
  "author": "pepedd864",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-eslint": "^9.0.5",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^20.12.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.45.0",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.7",
    "rollup": "^4.18.0",
    "rollup-plugin-dts": "^6.1.1",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.3",
    "typescript": "^5.3.3"
  }
}
